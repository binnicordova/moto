{
    "cli": {
        "version": ">= 16.1.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "development"
            },
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "preview"
            },
            "channel": "preview"
        },
        "production": {
            "channel": "production",
            "autoIncrement": true,
            "env": {
                "FIREBASE_API_KEY": "@eas:secret:FIREBASE_API_KEY_PRODUCTION",
                "FIREBASE_AUTH_DOMAIN": "@eas:secret:FIREBASE_AUTH_DOMAIN_PRODUCTION",
                "FIREBASE_PROJECT_ID": "@eas:secret:FIREBASE_PROJECT_ID_PRODUCTION",
                "FIREBASE_STORAGE_BUCKET": "@eas:secret:FIREBASE_STORAGE_BUCKET_PRODUCTION",
                "FIREBASE_MESSAGING_SENDER_ID": "@eas:secret:FIREBASE_MESSAGING_SENDER_ID_PRODUCTION",
                "FIREBASE_APP_ID": "@eas:secret:FIREBASE_APP_ID_PRODUCTION"
            }
        },
        "production_apk": {
            "extends": "production",
            "android": {
                "buildType": "apk"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
