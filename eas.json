{
    "cli": {
        "version": ">= 16.1.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "development"
            },
            "channel": "development"
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "APP_VARIANT": "preview"
            },
            "channel": "preview"
        },
        "production": {
            "channel": "production"
        }
    },
    "submit": {
        "production": {}
    }
}
